{
  "extends": [
    "./eslint/best-practices/walmart.js",
    "./eslint/errors/walmart.js",
    "./eslint/node/airbnb.js",
    "./eslint/style/airbnb.js",
    "./eslint/variables/airbnb.js",
    "./eslint/es6/airbnb.js"
  ],

  "rules": {
    "strict": [2, "global"],
    "no-magic-numbers": 0,
    "max-len": 0,
    "consistent-return": 0,
    "newline-after-var": 2
  },

  "env": {
    "es6": true,
    "node": true
  },

  "plugins": [
    "babel"
  ],

  "parser": "babel-eslint"
}
